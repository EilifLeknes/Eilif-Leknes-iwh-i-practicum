doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Pet table
        a(href="/update-cobj") Add to this table
        .cards 
            each pet in data
                .card 
                    h2.card__name #{pet.properties.name}
                    p.card__type #{pet.properties.type}
                    p.card__color #{pet.properties.color}